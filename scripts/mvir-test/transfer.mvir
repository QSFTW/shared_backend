import 0x01.LibraAccount;
import 0x01.Coin1;
import 0x01.Signer;
main ( payer: &signer, payee: address, amount: u64) 
{
    payer_withdrawal_cap = LibraAccount.extract_withdraw_capability(payer);
    
    LibraAccount.pay_from<Coin1>(&payer_withdrawal_cap ,move(payee), move(amount), x"", x"");
    return;
}
